{
  "env_name": "",
  "buffer":{
    "max_buffer_size": 1000000
  },
  "agent":{
    "gamma": 0.99,
    "update_target_network_interval": 1,
    "target_smoothing_tau": 0.005,
    "lambda": 0.75,
    "repeat_num": 10,
    "q_network":{
      "hidden_dims": [256, 256],
      "optimizer_class": "Adam",
      "learning_rate": 0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity"
    },
    "policy_network":{
      "hidden_dims": [256, 256],
      "optimizer_class": "Adam",
      "learning_rate": 0.0003,
      "act_fn": "relu",
      "out_act_fn": "identity",
      "phi": 0.05
    },
    "vae":{
      "hidden_dims": [750, 750],
      "optimizer_class": "Adam",
      "learning_rate": 0.001,
      "act_fn": "relu",
      "out_act_fn": "identity"
    }
  },
  "trainer":{
    "max_iteration": 60000,
    "num_updates_per_iteration": 50,
    "num_steps_per_iteration": 50,
    "update_n_interval": 50,
    "batch_size": 100,
    "max_trajectory_length":1000,
    "test_interval": 20,
    "num_test_trajectories": 5,
    "save_model_interval": 2000,
    "start_timestep": 1000,
    "start_use_policy": 0, 
    "save_video_demo_interval": 1000,
    "log_interval": 50,
    "sequential": false, 
    "log_gradient": false
  }
}
